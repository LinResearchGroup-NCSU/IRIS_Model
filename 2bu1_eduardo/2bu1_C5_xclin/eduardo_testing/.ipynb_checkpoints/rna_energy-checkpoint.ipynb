    "from modeller import *\n",
    "# Get the sequence of the PDB file, and write to an alignment file\n",
    "native_pdb = \"1hlo.pdb\"\n",
    "\n",
    "e = environ()\n",
    "m = model(e, file=native_pdb)\n",
    "aln = alignment(e)\n",
    "aln.append_model(m, align_codes=native_pdb)\n",
    "aln.write(file=native_pdb + '.seq')"
